<%= render partial: 'partials/system/form_errors', locals: { object: @score } %>

<%= form_with model: [:system, @score] do |form| %>

  <%= form.label :house_id %><br>
  <%= form.select :house_id, house_select_options, { prompt: 'Select' } %><br>

  <%= form.label :value %><br>
  <%= form.number_field :value, in: 0..12 %><br>

  <%= form.submit %>
<% end %>
